<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service id="sylius_mollie.grid.filter.mollie_logger_level" class="Sylius\MolliePlugin\Grid\Filter\MollieLoggerLevel">
            <tag name="sylius.grid_filter" type="log_level" form_type="Sylius\MolliePlugin\Form\Type\MollieLoggerLevelFilterType" />
        </service>

        <service id="sylius_mollie.grid.filter.mollie_subscription_state" class="Sylius\MolliePlugin\Grid\Filter\MollieSubscriptionState">
            <tag name="sylius.grid_filter" type="subscription_state" form_type="Sylius\MolliePlugin\Form\Type\MollieSubscriptionStateGridFilterType" />
        </service>
    </services>
</container>
